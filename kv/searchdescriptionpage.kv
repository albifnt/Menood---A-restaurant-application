<SearchDescriptionPage>:
    ScrollView:
        GridLayout:
            id: list_layout
            cols: 1
            spacing: dp(20)
            padding: dp(20)
            size_hint_y: None
            height: self.minimum_height
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
                Color:
                    rgba: [1,0,0,.5]
            GridLayout:
                rows: 1
                size_hint_y: None
                height: self.minimum_height
                MDIconButton:
                    icon: "keyboard-backspace"
                    pos_hint: {"center_y": 0.5, "left": 1}
                    on_release:
                        app.root.ids['firstpage'].ids['screen_manager_1'].transition.direction = 'right'
                        app.change_screen("screen_manager_1", "search_page")
                MDLabel:
                    id: name
                    text: root.name_place
                    font_style: 'H6'
                    italic: True
                    size_hint_y: None
                    halign: 'center'
                    valign: 'center'
                    text_size: self.width, None
                    height: dp(8) + self.texture_size[1] + dp(8)

            MDSeparator:
                id: sep_1

            GridLayout:
                cols: 1
                size_hint_y: None
                height: dp(220)
                SmallMapView:
                    id: small_map_view
                    lat: root.lat
                    lon: root.lon
                    SmallMapMarker:
                        lat: root.lat
                        lon: root.lon

            GridLayout:
                rows: 1
                size_hint_y: None
                height: dp(20)
                MDLabel:
                    text: "Indice Menood"
                    bold: True
                    italic: True
                    font_size: 30
                MDIconButton:
                    icon: 'map-marker'

            MDLabel:
                id: indice_menood
                text: "8.8/10"
                font_size: 26
                size_hint_y: None
                height: self.texture_size[1]

            GridLayout:
                rows: 1
                size_hint_y: None
                height: dp(20)
                MDLabel:
                    text: root.address_title
                    bold: True
                    italic: True
                    font_size: 30
                MDIconButton:
                    icon: 'map-marker'
                    on_release:
                        webbrowser.open("http://maps.apple.com/?address="+parse.quote(address_google_maps.text))
            MDLabel:
                id: address_google_maps
